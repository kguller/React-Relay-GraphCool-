{
  "name": "ZipBoss",
  "version": "5.0.0",
  "description": "ZipBoss Test Harness",
  "main": "index.js",
  "scripts": {
    "relay": "relay-compiler --src ./src --schema ./schema.graphql",
    "build": "relay-compiler --src ./src --schema ./schema.graphql && webpack-dev-server",
    "stage": "relay-compiler --src ./src --schema ./schema.graphql && webpack -p && gzip -9f < ./dist/bundle.js > ./dist/bundle.js.gz && gzip -9f < ./dist/bundle.js > ./dist/bundle.js.dgz",
    "schema": "get-graphql-schema https://api.graph.cool/relay/v1/cj6il7hon3n290110qpmr1zhq > schema.graphql",
    "deploy": "yarn run stage && echo 'deployed'"
  },
  "repository": "zipboss.harness",
  "author": "Kieran Guller",
  "license": "MIT",
  "dependencies": {
    "@shopify/polaris": "^1.1.1",
    "@trendmicro/react-toggle-switch": "^0.5.5",
    "add": "^2.0.6",
    "auth0-lock": "^10.15.1",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^6.1.2",
    "classnames": "^2.2.5",
    "eslint": "^3.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-flowtype": "^2.15.0",
    "eslint-plugin-react": "^5.2.2",
    "form-serialize": "^0.7.1",
    "get-graphql-schema": "^2.0.0",
    "html-to-react": "^1.2.7",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "rc-tooltip": "^3.4.7",
    "react": "^15.5.4",
    "react-color": "^2.13.3",
    "react-dock": "^0.2.4",
    "react-dom": "^15.5.4",
    "react-dropzone": "^3.13.3",
    "react-modal": "^1.7.7",
    "react-moment": "^0.6.0",
    "react-relay": "^1.0.0",
    "react-router-dom": "^4.1.1",
    "relay-compiler": "^1.2.0",
    "relay-runtime": "^1.2.0",
    "serialize-javascript": "^1.3.0",
    "webfontloader": "^1.6.28",
    "webpack": "^2.4.1",
    "yarn": "^0.27.5"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-relay": "^1.2.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.0",
    "script-loader": "^0.7.0",
    "style-loader": "^0.16.1",
    "webpack-dev-server": "2"
  }
}
